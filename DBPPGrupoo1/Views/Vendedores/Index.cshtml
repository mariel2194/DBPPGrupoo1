@model IEnumerable<DBPPGrupoo1.Vendedores>

@{
    ViewBag.Title = "Index";
}

<div class="custom" style="width:100px;">
    <h2>Registro de Vendedores</h2>
    <br />



    <div class="btn-text-left">
        <button type="button" class="btn"> @Html.ActionLink("Create New", "Create")</button>
    </div>

    <br />
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VendedorId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ComisionPorVenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Activo)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VendedorId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Apellido)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ComisionPorVenta)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Activo)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.VendedorId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.VendedorId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.VendedorId })
                </td>
            </tr>
        }

    </table>
</div>